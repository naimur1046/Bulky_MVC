@model List<Category>

<div>
    <h1>
        Category List
    </h1>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>
                    Category ID
                </th>
                <th>
                    Category Name
                </th>
                <th>
                    Category Display Order
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Id
                    </td>
                    <td>
                      
                        @obj.Name

                    </td>
                    <td>
                        @obj.DisplayOrder
                    </td>
                </tr>
            }
        </tbody>
    </table>
    
</div>